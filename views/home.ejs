<%- include('includes/header.ejs') %>
.quiz-data{
    background-color: rgb(178, 174, 229);
    padding: 10px;
    border-radius: 10px;
    margin-top: 10px;
}
.quiz-data a{
    display: inline-block;
    background-color: rgb(47, 36, 165);
    color: white;
    text-decoration: none;
    padding: 7px;
    border-radius: 10px;
    margin-top: 20px;
}

.quiz-data strong{
    margin-right: 3px;
}
#quiz-container{
    padding: 10px;
}
<%- include('includes/nav.ejs') %>
    <div id="quiz-container">
        <% for(let quiz of quizList){%>
            <div class="quiz-data">
                <p><strong>Title:</strong><%= quiz.quizTitle %></p>
                <p><strong>Description:</strong>  <%= quiz.quizDes %></p>
                <a href="/play-quiz/<%= quiz.id %>">PLAY QUIZ</a>
            </div>
        <% } %>
    </div>
</body>
</html>